#ifndef INC_KALMAN_FNS_H_
#define INC_KALMAN_FNS_H_

#define float alpha = 0.98;
#define dt = 0.005;

struct {
  float pitch;
  float roll;
  float yaw;
} Orientation;

Orientation updateOrientation(float accX, float accY, float accZ, float gyroX, float gyroY, float gyroZ);

